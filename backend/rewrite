GET /users/		getAllUsers()
GET /users/:id		getUserById()
GET /users/:id/favorite	getUsersFavorites()
GET /books/		getAllBooks()
GET /books/:id		getBookById()
POST /users/register	addNewUser()		doesUserExist()
POST /users/login	getPassword()
POST /books/		addNewBook()		doesBookExist()
POST /books/:id/fav-	addBookToUsersFavorite() //include checking if book is already in fav
DELETE /users/:id	deleteUser()
DELETE /books/:id	deleteBook()

